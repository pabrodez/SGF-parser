{
  "name": "ts-sgf-parser",
  "version": "0.2.2",
  "description": "A parser for SGF written in Typescript",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/ts-sgf-parser.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.umd.js",
      "import": "./dist/index.esm.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "bundle": "rollup -c rollup.config.js",
    "test": "jest",
    "type-check": "tsc --noEmit",
    "build:types": "tsc -t esnext --moduleResolution node -d --emitDeclarationOnly --outFile ./dist/ts-sgf-parser.d.ts ./src/ts-sgf-parser.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pabrodez/SGF-parser.git"
  },
  "author": "Pablo M. Rodriguez <pabrodez@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/pabrodez/SGF-parser",
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.1.0",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.16",
    "jest": "^26.6.3",
    "rollup": "^2.35.1",
    "ts-jest": "^26.4.4",
    "tslib": "^2.0.3",
    "typescript": "^4.1.3"
  }
}
